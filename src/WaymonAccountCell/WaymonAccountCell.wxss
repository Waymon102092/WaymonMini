.waymon_account_cell{
  float: left;
  padding-left: 20rpx;
  margin-top: 20rpx;
  width: calc(30% - 20rpx);
  margin-left: 2.5%;
  height: 100rpx;
  background: #eee;
  border-radius: 10rpx;
}
.waymon_account_cell view:first-child{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.waymon_account_cell view:last-child{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #999999;
}